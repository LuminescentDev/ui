  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.3",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "tailwindcss": "3.4.10",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.31.1"
