{
  "name": "@luminescent/ui",
  "version": "1.4.6-1",
  "description": "Luminescent UI library",
  "main": "./lib/index.js",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib-types/index.d.ts"
    },
    "./components/*": {
      "import": "./lib/components/*",
      "require": "./lib/components/*",
      "types": "./lib-types/components/*"
    },
    "./css": "./src/index.css"
  },
  "files": [
    "lib",
    "lib-types",
    "src/index.css"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "license": "AGPL-3.0-or-later",
  "private": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm lint && tsc",
    "dev": "echo \"No dev script specified\" && exit 0",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "echo \"No test specified\" && exit 0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.3",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "tailwindcss": "3.4.10",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.31.1"
  }
}
